{% extends 'base.html.twig' %}

{% block title %}CollaborationRequest index{% endblock %}

{% block body %}
    <h1>CollaborationRequest index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for collaboration_request in collaboration_requests %}
            <tr>
                <td>{{ collaboration_request.id }}</td>
                <td>
                    <a href="{{ path('app_collaboration_request_show', {'id': collaboration_request.id}) }}">show</a>
                    <a href="{{ path('app_collaboration_request_edit', {'id': collaboration_request.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_collaboration_request_new') }}">Create new</a>
{% endblock %}
